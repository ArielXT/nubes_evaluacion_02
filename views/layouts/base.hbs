                      <div class="divider-custom">
                        <div class="divider-custom-line"></div>
                        <div class="divider-custom-icon"><i class="fas fa-bolt"></i></div>
                        <div class="divider-custom-line"></div>
                      </div>
                      <img class="img-fluid rounded mb-5" src="../img/type.gif" alt="..." />
